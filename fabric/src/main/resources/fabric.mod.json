{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${website}",
    "sources": "${github}",
    "issues": "${issue_tracker}"
  },
  "license": "${mod_license}",
  "icon": "${mod_logo}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "fr.foxelia.igtips.fabric.InGameTipsFabric"
    ],
    "client": [
      "fr.foxelia.igtips.fabric.client.InGameTipsFabricClient"
    ],
    "modmenu": [
      "fr.foxelia.igtips.client.addons.modmenu.InGameTipsModMenuIntegration"
    ]
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord}",
        "modmenu.modrinth": "${modrinth}"
      }
    }
  },
  "mixins": [
    "fabric.mixins.json",
    "igtips.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${depend_fabric}",
    "minecraft": "~${minecraft_version}",
    "java": ">=17",
    "architectury": ">=${depend_architectury}",
    "fabric-api": "*",
    "forgeconfigapiport": ">=${depend_fuzs_config}"
  }
}
