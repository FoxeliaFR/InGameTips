{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "contributors": {
        "${mod_author}": "${mod_contributors}"
      },
      "contact": {
        "homepage": "${website}",
        "sources": "${github}",
        "issues": "${issue_tracker}"
      },
      "license": "${mod_license}",
      "icon": "${mod_logo}"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "fr.foxelia.igtips.quilt.InGameTipsQuilt"
      ],
      "client_init": [
        "fr.foxelia.igtips.quilt.client.InGameTipsQuiltClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.2.14"
      }
    ]
  },
  "modmenu": {
    "links": {
      "modmenu.discord": "${discord}",
      "modmenu.modrinth": "${modrinth}"
    }
  },
  "mixin": "quilt.mixins.json"
}
